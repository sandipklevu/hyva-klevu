<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="content">
            <block class="Magento\Framework\View\Element\Template"
                   template="Klevu_HyvaPage::theme-landing.phtml"
                   after="klevu-js-core"
                   name="search_index_klevutemplate.theme-defer"
                   ifconfig="klevu_search/general/enabled">
                <arguments>
                    <argument name="view_model" xsi:type="object">Klevu\HyvaPage\ViewModel\JSFileSourceDataProvider
                    </argument>
                </arguments>
            </block>
        </referenceBlock>
    </body>
</page>
